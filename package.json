{
  "name": "dyno-table",
  "version": "0.1.4",
  "description": "A TypeScript library to simplify working with DynamoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:w": "vitest watch",
    "test:int": "vitest --config vitest.integration.ts",
    "lint": "biome lint",
    "check-types": "tsc --noEmit",
    "ddb:start": "docker compose up -d dynamodb"
  },
  "keywords": ["dynamodb", "aws", "typescript", "database"],
  "author": "Maunder",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kysumi/dyno-table.git"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@biomejs/biome": "1.9.4",
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0"
  },
  "files": ["dist"],
  "packageManager": "pnpm@10.5.2"
}
